import os
import time
x2 = "xy"
print(x2)
while True:
  os.system('clear')
  x2 = ">---<"
  print(x2)
  time.sleep(.1)
  os.system('clear')
  x2 = "->-<-"
  print(x2)
  time.sleep(.1)
  os.system('clear')
  x2 = "--|--"
  print(x2)
  time.sleep(.1)
  os.system('clear')
  x2 = "-<->-"
  print(x2)
  time.sleep(.1)
  os.system('clear')
  x2 = "<---->"
  print(x2)
  time.sleep(.1)
